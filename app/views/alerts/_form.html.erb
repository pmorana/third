<%= simple_form_for(@alert) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :alert_set %>
    <%= f.association :user %>
    <%= f.association :event %>
    <%= f.input :alert_name %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
